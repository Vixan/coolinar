{
  "name": "coolinar-server",
  "version": "0.0.0",
  "description": "Server-side for the coolinar application.",
  "author": "Duca Vitalie-Alexandru",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:services:init-db": "./src/scripts/start-db.sh",
    "start:services": "docker-compose up -d && exit 0",
    "stop:services": "docker-compose kill",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "start:sonar": "docker-compose -f docker-compose.sonar.yml up -d",
    "stop:sonar": "docker-compose up -d && exit 0",
    "sonar": "sonarqube-verify"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.11.8",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.11.8",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.8",
    "@nestjs/typeorm": "^6.3.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.0",
    "dotenv": "^6.2.0",
    "helmet": "^3.21.2",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.4",
    "slug": "^1.0.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/hapi__joi": "^16.0.11",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/passport-local": "^1.0.33",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^25.0.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "sonarqube-scanner": "^2.8.0",
    "sonarqube-verify": "^1.0.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
